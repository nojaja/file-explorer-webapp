{
  "providers": {
    "a.localhost:3000": {
      "github": {
        "id": "github",
        "name": "Github",
        "CLIENT_ID": "YOUR_GITHUB_CLIENT_ID",
        "CLIENT_SECRET": "YOUR_GITHUB_CLIENT_SECRET",
        "CALLBACK_URL": "http://a.localhost/oauth/github/callback",
        "enabled": false
      },
      "gitlab": {
        "id": "gitlab",
        "name": "GitLab",
        "GITLAB_CLIENT_ID": "your-client-id-a",
        "GITLAB_CLIENT_SECRET": "your-secret-a",
        "GITLAB_CALLBACK_URL": "http://a.localhost/oauth/callback",
        "GITLAB_URL": "https://gitlab.default.com",
        "GITLAB_URL_INTERNAL": "http://gitlab-internal-a:80",
        "GITLAB_TOKEN_URL_INTERNAL": "https://gitlab.default.com/oauth/token",
        "GITLAB_USERINFO_URL_INTERNAL": "https://gitlab.default.com/api/v4/user",
        "enabled": true
      },
      "google": {
        "id": "google",
        "name": "Google",
        "CLIENT_ID": "your-google-client-id-a",
        "CLIENT_SECRET": "your-google-secret-a",
        "CALLBACK_URL": "http://a.localhost/oauth/google/callback",
        "enabled": false
      },
      "hydra": {
        "id": "hydra",
        "name": "Hydra",
        "HYDRA_CLIENT_ID": "your-hydra-client-id-a",
        "HYDRA_CLIENT_SECRET": "your-hydra-secret-a",
        "HYDRA_CALLBACK_URL": "http://a.localhost/oauth/hydra/callback",
        "HYDRA_AUTH_URL": "http://hydra-a:4444/oauth2/auth",
        "HYDRA_TOKEN_URL_INTERNAL": "http://hydra-a:4444/oauth2/token",
        "HYDRA_ADMIN_URL": "http://localhost:4445",
        "HYDRA_ADMIN_URL_INTERNAL": "http://hydra-a:4445",
        "HYDRA_USERINFO_URL_INTERNAL": "http://hydra:4444/userinfo",
        "HYDRA_SCOPE": [
          "openid",
          "profile",
          "email"
        ],
        "enabled": true
      }
    },
    "b.localhost:3000": {
