{
  "name": "file-explorer-webapp",
  "version": "0.1.0",
  "description": "Web UIでファイル・フォルダ操作ができるNode.jsアプリ（Docker対応）",
  "main": "src/backend/index.js",
  "type": "module",
  "scripts": {
    "start": "docker-compose up -d --force-recreate --build app",
    "start:local": "node src/backend/index.js",
    "build:frontend": "webpack --config webpack.frontend.config.js",
    "build:backend": "webpack --config webpack.backend.config.js",
    "build": "npm run lint; npm run jscpd; npm run build:frontend ; npm run build:backend ; ",
    "test": "jest",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "jscpd": "jscpd --config .jscpd.json"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "archiver": "^7.0.0",
    "check-disk-space": "^3.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.17.3",
    "handlebars": "^4.7.8",
    "multer": "^2.0.1",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-gitlab2": "^2.0.0",
    "passport-oauth2": "^1.8.0",
    "pkg": "^5.8.1",
    "promised-handlebars": "^2.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@playwright/test": "^1.52.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.4",
    "playwright": "^1.43.1",
    "path-browserify": "^1.0.1",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1",
    "eslint": "^8.45.0",
    "jscpd": "^3.5.0"
  }
}